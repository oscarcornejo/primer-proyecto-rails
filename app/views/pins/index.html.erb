<p id="notice"><%= notice %></p>

<h1>Lista de Pins</h1>

    <% @pins.each do |pin| %>
      <div class="col-md-3">
        <div class="thumbnail">
        
        <%= pin.name %>
        <%= image_tag(pin.photo) %>
        <%= pin.address %>
        <%= link_to 'Show', pin %>
        <%= link_to 'Edit', edit_pin_path(pin) %>
        <%= link_to 'Destroy', pin, method: :delete, data: { confirm: 'Are you sure?' } %>
        <% end %>

      </div>
    </div>
     

<%= link_to 'New Pin', new_pin_path %>
